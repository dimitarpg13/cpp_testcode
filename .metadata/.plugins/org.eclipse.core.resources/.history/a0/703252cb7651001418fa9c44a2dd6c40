#ifndef COMPANYINFO_H_
#define COMPANYINFO_H_

#include <string>

using namespace std;

namespace Algorithms {

class CompanyInfo {
public:
	CompanyInfo(string name);
	virtual ~CompanyInfo();
    string getName() { return m_sName; };

private:
	string m_sName;
};

} /* namespace Algorithms */

#endif /* COMPANYINFO_H_ */
