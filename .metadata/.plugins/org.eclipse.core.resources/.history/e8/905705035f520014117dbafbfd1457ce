  }

  LCS::print(vector<int> s)
  {
      vector<int> lcs, buf;
      bool asc = false;
      bool desc = false;
      for (int i = 0; i < s.size(); i++)
      {
    	  if (s[i+1] - s[i] == 1)
    	  {
             if (desc) // this is a new sequence so reset the buffer
             {
            	if (buf.size() > lcs.size())
            	{

            		lcs.assign(buf.begin(),buf.end());
            	}
             }
    	  }
    	  else if (s[i] - s[i+1] == 1)
    	  {

    	  }
    	  else
    	  {

    	  }
      }


  }


} /* namespace Algorithms */
