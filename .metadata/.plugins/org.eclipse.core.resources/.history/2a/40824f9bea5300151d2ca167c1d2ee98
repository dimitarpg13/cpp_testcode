#include "ProblemParser.h"

ProblemParser::ProblemParser(char sep, int symb_count, char symbols[])
{

}

bool ProblemParser::is_symbol(char c)
{
	if (c >= '1' && c <= '9')
		return true;
	else
		return false;
}

bool ProblemParser::is_separator(char c)
{
   return true;
}


bool parse(string inputFile)
{
	ifstream fs (inputFile.c_str(), std::ifstream::in);

	char c = fs.get();
	while (fs.good())
	{
	  if ( c >= '1' && c <= '9' )
	  {
	     std::cout << c;
	     c = fs.get();
	  }
	}

	fs.close();

	return false;
}
