#include "RBT.h"

namespace Algorithms {

RBT::RBT(int cmp(const void* p, const void* q)) {
    _cmp = cmp;
    _root = nullptr;

}

RBT::~RBT() {
	if (_root != nullptr)
	   delete _root;
}

bool RBT::add(const void* pdata)
{
	bool res = true;

	if (_root = nullptr)
	{
		_root = new RBTNode(pdata);
	}
	return res;
}




RBTNode::RBTNode()
{
	value = nullptr;
	p = nullptr;
	left = nullptr;
	right = nullptr;
	color = Color::Black;
}

RBTNode::RBTNode(const void* pdata, Color col)
{
	value = pdata;
	p = nullptr;
	left = nullptr;
	right = nullptr;
	color = col;
}

} /* namespace Algorithms */

