#include <iostream>
#include <fstream>

#include "Parser.h"
#include "Symbol.h"
#include "Problem.h"

using namespace std;
using namespace sudoku;

int main() {
	cout << "SudokuQlik" << endl; // prints SudokuQlik


    char symbols[] = {'1','2','3','4','5','6','7','8','9'};
	Parser parser('.',9,symbols);

	parser.parse("./src/example1.txt");

    Symbol s('1');
    Region * r = s.getRegion();
    vector<Region *> v;
    v.push_back(r);

    Problem * p = new Problem(9);
    delete p;

    char c = 0101;
    cout << c ;

	return 0;
}
