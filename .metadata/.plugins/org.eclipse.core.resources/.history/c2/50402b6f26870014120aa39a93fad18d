#include <iostream>
#include <type_traits>

using namespace std;

typedef int t1;
typedef add_pointer<t1>::type A; 	//int*


int main() {
	cout << "add_pointer example" << endl; // prints add_pointer example
	A var1 = new t1[2];

	return 0;
}
