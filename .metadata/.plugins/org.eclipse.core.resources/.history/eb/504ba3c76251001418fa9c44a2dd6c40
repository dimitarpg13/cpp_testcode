#ifndef INDUSTRYCOMP_H_
#define INDUSTRYCOMP_H_

#include "IndustryInfo.h"

namespace Algorithms {

struct IndustryComp {
  bool operator() (IndustryInfo& lh, IndustryInfo& rh) const
  {   return  lh.getName().compare(rh.getName()) < 0 };
};

} /* namespace Algorithms */

#endif /* INDUSTRYCOMP_H_ */
