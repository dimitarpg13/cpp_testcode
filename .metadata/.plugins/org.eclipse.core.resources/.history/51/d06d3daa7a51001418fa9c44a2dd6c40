#include "CompanyInfo.h"

namespace Algorithms {

CompanyInfo::CompanyInfo(string name, string industry, float marketCap, float revenue)
{
    m_sName = name;
	m_sIndustry = industry;
	m_fMarketCap = marketCap;
	m_fRevenue = revenue;
}

CompanyInfo::~CompanyInfo() {
	// TODO Auto-generated destructor stub
}

string CompanyInfo::getName() { return m_sName; };

string CompanyInfo::getIndustry() { return m_sIndustry; };

float CompanyInfo::getMarketCap() { return m_fMarketCap; };

float CompanyInfo::getRevenue() { return m_fRevenue; };

} /* namespace Algorithms */
