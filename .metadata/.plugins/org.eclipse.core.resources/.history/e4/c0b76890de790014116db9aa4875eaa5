#include "RBT.h"

namespace Algorithms {

RBT::RBT(int cmp(const void* p, const void* q)) {
    _cmp = cmp;
    _root = nullptr;
    _nil = nullptr;
}

RBT::~RBT() {
	if (_root != nullptr)
	   delete _root;
	if (_nil != nullptr)
	   delete _nil;
}

bool RBT::add(const void* pdata)
{
	bool res = true;

	return res;
}




RBTNode::RBTNode()
{
	value = nullptr;
	p = nullptr;
	left = nullptr;
	right = nullptr;
	color = Color::Black;
}

} /* namespace Algorithms */

