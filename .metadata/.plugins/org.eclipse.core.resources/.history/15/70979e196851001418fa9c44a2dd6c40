#ifndef INDUSTRYCOMP_H_
#define INDUSTRYCOMP_H_

#include <string>
#include "IndustryInfo.h"

using namespace std;

namespace Algorithms {

struct IndustryComp {
    bool operator() (const IndustryInfo  & lhs, const IndustryInfo & rhs) {
    	 if (lhs.getName().compare(rhs.getName()) < 0)
    		   return true;
    	   else
    		   return false;
    }
};

} /* namespace Algorithms */

#endif /* INDUSTRYCOMP_H_ */
