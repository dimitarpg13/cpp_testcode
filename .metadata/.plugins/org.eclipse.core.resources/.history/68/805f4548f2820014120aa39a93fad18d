#include <iostream>
using namespace std;

int integer_comp(int * l, int * r)
{
	if (l != nullptr && r != nullptr)
	{
		if (*l > *r)
			return 1;
		else if (*l < *r)
			return -1;
		else
			return 0;
	}
	else
		return 0;
}

int main() {


	return 0;
}
