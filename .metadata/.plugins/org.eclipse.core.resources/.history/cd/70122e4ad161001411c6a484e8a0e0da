#include <iostream>
#include <algorithm>
#include <vector>
#include <map>

using namespace std;

//
//  Split a set of integers into two sets with the same sums (if exists)
//
int main() {

    vector<int> vec;

    vec.push_back(10);
    vec.push_back(11);
    vec.push_back(7);

    sort(vec.begin(), vec.end());

    for (vector<int>::iterator it = vec.begin(); it != vec.end(); it++)
    {
       cout << *it << endl;
    }




	return 0;
}
