#include <iostream>
#include <fstream>

using namespace std;

int main() {
	cout << "SudokuQlik" << endl; // prints SudokuQlik

	ifstream fs ("../example1.txt", std::ifstream::in);
	char c = fs.get();

	while (fs.good()) {
	   std::cout << c;
	   c = fs.get();
	}

	fs.close();

	return 0;
}
