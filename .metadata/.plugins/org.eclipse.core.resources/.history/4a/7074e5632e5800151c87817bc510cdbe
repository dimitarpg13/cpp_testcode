#include <iostream>
#include <algorithm>
#include <iterator>
#include <set>


using namespace std;

int main() {

	cout << "TestSudoku" << endl; // prints TestSudoku

    std::set<char> s1, s2;
	s1.insert('1');
	s1.insert('2');
	s1.insert('3');
	s1.insert('4');
	s1.insert('5');
	s2.insert('2');
	s2.insert('3');
	const set<char> & s11 = s1;

	// Fill in s1 and s2 with values
	std::set<char> result;
	std::set_difference(s11.begin(), s11.end(), s2.begin(), s2.end(),
	    std::inserter(result, result.end()));

	for (set<char>::iterator it = result.begin(); it != result.end(); it++)
		cout << *it;
	return 0;
}
