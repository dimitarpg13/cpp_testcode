#include <iostream>
#include "RBT.h"

using namespace std;
using namespace Algorithms;

using pint = const int*;

int integer_comp(const void * l, const void * r)
{
	if (l != nullptr && r != nullptr)
	{
		pint lv = pint{l};
		pint rv = pint{r};
		if (*lv > *rv)
			return 1;
		else if (*lv < *rv)
			return -1;
		else
			return 0;
	}
	else
		return 0;
}

int main() {


    RBT rbt(&integer_comp);

	return 0;
}
