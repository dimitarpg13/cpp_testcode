#include "Car.h"

namespace Algorithms {

Car::Car(string brand) {
    this->brand = brand;
}

Car::~Car() {

}

int Car::getPassengerCount()
{
	return passengerCount;
}

void Car::setPassengerCount(int val)
{
	passengerCount = val;
}

string Car::getLicensePlate()
{
	return licensePlate;
}

void Car::setLicensePlate(string val)
{
	licensePlate = val;
}

string Car::getBrand()
{
	return brand;
}

} /* namespace Algorithms */
