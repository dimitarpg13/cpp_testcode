#include <iostream>
#include <string>

#include <boost/bind.hpp>

std::string f(std::string const & x)
{
	return "f(" + x + ")";
}

std::string g(std::string const & x)
{
	return "g(" + x + ")";
}




int main() {
	std::cout << "Boost bind examples" << std::endl; // prints Boost bind examples


	return 0;
}
