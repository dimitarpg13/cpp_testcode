#include <iostream>
using namespace std;

enum class Kind : char {
	name, number, end,
	plus='+', minus='-', mul='*', div='/', print=';',
	assign='=', lp='(', rp=')'
};

struct Token {
	Kind kind;
	string string_value;
	double number_value;
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
