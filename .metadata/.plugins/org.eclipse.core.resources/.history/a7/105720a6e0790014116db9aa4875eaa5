#include "RBT.h"

namespace Algorithms {

RBT::RBT(int cmp(const void* p, const void* q)) {
    _cmp = cmp;
    _root = nullptr;

}

RBT::~RBT() {
	if (_root != nullptr)
	   delete _root;
}

RBTNode * RBT::find_pred(const void* val)
{
	RBTNode * res = nullptr;

	return res;
}

bool RBT::add(const void* val)
{
	bool res = true;

	if (_root = nullptr)
	{
		_root = new RBTNode(val,Color::Black);
	}
	else
	{
       RBTNode * predecessor = find_pred(val);

	}

	return res;
}




RBTNode::RBTNode()
{
	value = nullptr;
	p = nullptr;
	left = nullptr;
	right = nullptr;
	color = Color::Black;
}

RBTNode::RBTNode(const void* pdata, Color col)
{
	value = pdata;
	p = nullptr;
	left = nullptr;
	right = nullptr;
	color = col;
}

} /* namespace Algorithms */

