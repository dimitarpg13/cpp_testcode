#ifndef SYMBOL_H_
#define SYMBOL_H_

#include "Region.h"
#include "Line.h"

namespace sudoku
{

class Symbol
{
private:
	Region * m_pRegion;
	HorizLine * m_pHorizLine;
	VertLine * m_pVertLine;

public:
	Region const * getRegion() { return m_pRegion; };
	HorizLine const * getHorizLine() { return m_pHorizLine; }
	VertLine const * getVertLine() { return m_pVertLine; }


};

}

#endif /* SYMBOL_H_ */
