#include "IndustryInfo.h"

namespace Algorithms {

IndustryInfo::IndustryInfo(string name)  {

	this->sName = name;
	this->sChildren = new set<IndustryInfo*,bool(*)(IndustryInfo* lhs, IndustryInfo* rhs)>(compare);

}

IndustryInfo::~IndustryInfo() {
	// TODO Auto-generated destructor stub
}

bool IndustryInfo::compare (IndustryInfo* lhs, IndustryInfo* rhs)
{
   if (lhs->getName().compare(rhs->getName()) < 0)
	   return true;
   else
	   return false;
}

} /* namespace Algorithms */

