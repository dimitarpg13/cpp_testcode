#include <iostream>
using namespace std;
int main() {
	cout << "HackerRank tests" << endl; // prints HackerRank tests
	int n;

	cin >> n;

	int v[n], i = 0;
    for (i=0; i<n; i++)
    {
        cin >> v[i] ;
    }

    int maxDiff = -1,
    	maxCnt = 0,
        mostPopularDiff = -1,
        mostPopularCnt = 0,
        curDiff = -1;


    for (i = 0; i < n-1; i++)
    {
       curDiff = v[i+1] - v[i];
      // if (curDiff > minDiff)
    }



    int missingTerm = -1;




	cout << missingTerm;

	return 0;
}
