#include <iostream>
#include <string>
#include <algorithm>
#include <vector>
#include <set>
#include <map>

using namespace std;

void afunc(int * iptr)
{
	*iptr=102;
}

int main() {

	int i = 101;
	afunc(&i);
	cout << "*i=" << i << endl;

	return 0;
}
