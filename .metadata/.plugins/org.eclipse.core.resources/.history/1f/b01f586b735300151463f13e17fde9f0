#include <iostream>
#include <fstream>

using namespace std;

int main() {
	cout << "SudokuQlik" << endl; // prints SudokuQlik

	ifstream fs ("./src/example1.txt", std::ifstream::in);
	char c = fs.get();


	while (fs.good())
	{
	  if ( c >= '0' && c <= '9' )
	  {
	     std::cout << c;
	     c = fs.get();
	  }
	}

	fs.close();

	return 0;
}
