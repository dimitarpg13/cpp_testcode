#ifndef RBT_H_
#define RBT_H_

namespace Algorithms {

enum class Color
{
	Red=1,
	Black=2
};

class RBTNode
{
	void* value;
	Color color;
};

class RBT {
public:
	RBT();
	RBT(int (*)(const void* p, const void* q) )

	virtual ~RBT();
private:
	int _cmp(const void* p, const void* q);
};

} /* namespace Algorithms */

#endif /* RBT_H_ */
