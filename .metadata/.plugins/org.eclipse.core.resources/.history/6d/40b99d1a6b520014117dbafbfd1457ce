#ifndef CAR_H_
#define CAR_H_

namespace Algorithms {

class Car {
public:
	Car(string brand);
	virtual ~Car();
	int getPassengerCount();
	void setPassengerCount(int);
	string getLicensePlate();
	void setLicensePlate(string);
	string getBrand();
private:
	int passengerCount;
	string licensePlate;
	string brand;
};

} /* namespace Algorithms */

#endif /* CAR_H_ */
